@model Build_a_PC_Sales_Deal_Hunter.Models.StatusInfoModel
@{
    ViewBag.Title = "Stats";
}

<script type="text/javascript">
        $(document).ready(function() {
            var table = $('#dataTable').DataTable(
            {
                "info": false
            });
        });
    </script>

<h2>Stats</h2>
@if (Model.UniqueUsers != null && Model.Errors != null)
{
    <p>@Model.UniqueUsers Unique users</p>

    <table id="dataTable" border="1" cellpadding="10">
        <thead><tr><th>Time</th><th>Error Message</th></tr></thead>
        <tbody>
            @foreach (var item in Model.Errors)
            {
                <tr>
                    <td>@item.Time</td>
                    <td>@item.ErrorMessage</td>
                </tr>
            }
        </tbody>
    </table>
}
